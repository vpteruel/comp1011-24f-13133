<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.vinicius.rickandmorty.controllers.SearcherController"
            prefHeight="648.0" prefWidth="824.0">
    <children>
        <HBox prefHeight="58.0" prefWidth="824.0" spacing="16.0" style="-fx-background-color: gray;">
            <children>
                <TextField fx:id="autocompleteTextField" prefHeight="26.0" prefWidth="716.0"
                           promptText="character name"/>
                <Button mnemonicParsing="false" text="Search"/>
            </children>
            <padding>
                <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
            </padding>
        </HBox>
        <Pane fx:id="contentPane" layoutY="58.0" prefHeight="590.0" prefWidth="824.0"/>
    </children>
</AnchorPane>
